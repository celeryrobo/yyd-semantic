#YBNF 1.0 utf8;
#include original.ybnf;
#include classpath:semantics/commons/common.ybnf;
#include classpath:semantics/commons/story.ybnf;
service story;
root $main;

$story_case1{action%queryResource} = 这个[故事]不好听|我不喜欢这个[故事];
$story_case2{action%queryResource} = ($case1|$case2) [$intent] [一个][个];
$story_main = $story_case1|$story_case2;

$main{action%story} = $story_common|$story_main;